{
  "name": "electron-forge-maker-innosetup",
  "version": "0.2.9",
  "description": "An Electron Forge maker for creating Windows installers using Innosetup with full TypeScript support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "node dist/index.js",
    "setup-portable": "node scripts/setup-portable.js",
    "prepare": "npm run build",
    "preversion": "npm test",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@electron-forge/maker-base": "^7.0.0",
    "@electron-forge/shared-types": "^7.0.0",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "vendor/**/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "electron",
    "electron-forge",
    "maker",
    "innosetup",
    "installer",
    "windows",
    "setup",
    "typescript"
  ],
  "author": "xunrua",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/promisemey/electron-forge-maker-innosetup.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
